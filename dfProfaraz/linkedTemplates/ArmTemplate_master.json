{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfProfaraz"
		},
		"Azure_sql_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Azure_sql_ls'"
		},
		"Storage_Acc_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Storage_Acc'"
		},
		"Azure_sql_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dfprofaraz.database.windows.net"
		},
		"Azure_sql_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dfprodb"
		},
		"Azure_sql_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "faraz"
		},
		"Storage_Acc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azurestgpro.dfs.core.windows.net/"
		},
		"ls_restapi_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pokeapi.co/api/v2/pokemon"
		},
		"schedule_properties_scheduled_parameters_schema": {
			"type": "string",
			"defaultValue": "source"
		},
		"schedule_properties_scheduled_parameters_table": {
			"type": "string",
			"defaultValue": "Orders"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfProfaraz_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Azure_sql_ls_password": {
						"value": "[parameters('Azure_sql_ls_password')]"
					},
					"Storage_Acc_accountKey": {
						"value": "[parameters('Storage_Acc_accountKey')]"
					},
					"Azure_sql_ls_properties_typeProperties_server": {
						"value": "[parameters('Azure_sql_ls_properties_typeProperties_server')]"
					},
					"Azure_sql_ls_properties_typeProperties_database": {
						"value": "[parameters('Azure_sql_ls_properties_typeProperties_database')]"
					},
					"Azure_sql_ls_properties_typeProperties_userName": {
						"value": "[parameters('Azure_sql_ls_properties_typeProperties_userName')]"
					},
					"Storage_Acc_properties_typeProperties_url": {
						"value": "[parameters('Storage_Acc_properties_typeProperties_url')]"
					},
					"ls_restapi_properties_typeProperties_url": {
						"value": "[parameters('ls_restapi_properties_typeProperties_url')]"
					},
					"schedule_properties_scheduled_parameters_schema": {
						"value": "[parameters('schedule_properties_scheduled_parameters_schema')]"
					},
					"schedule_properties_scheduled_parameters_table": {
						"value": "[parameters('schedule_properties_scheduled_parameters_table')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}