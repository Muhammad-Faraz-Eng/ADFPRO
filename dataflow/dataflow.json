{
	"name": "dataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_csv_dynamic",
						"type": "DatasetReference"
					},
					"name": "ingestion"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "Storage_Acc",
						"type": "LinkedServiceReference"
					},
					"name": "sink",
					"rejectedDataLinkedService": {
						"referenceName": "Storage_Acc",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          booking_id as string,",
				"          passenger_id as string,",
				"          flight_id as string,",
				"          airport_id as string,",
				"          amount as double,",
				"          booking_date as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> ingestion",
				"ingestion sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'sink',",
				"     folderPath: 'deltaState',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['booking_id'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink"
			]
		}
	}
}